//Process request to change Windows Speech Recognition (WSR) profile using C# inline function that reads from and writes to the Windows registry

using System;
using System.Linq;
using Microsoft.Win32;
using System.Windows.Forms;

public class VAInline
{
    public void main()
    {
        //Initialize important variables
        //Go through these and place in more appropriate spots in the code
        string ActivatedProfileName = VA.GetText("WSRActivatedProfile"); //Profile name that user wants to activate, passed from VoiceAttack
        string ProfileNameString = VA.GetText("WSRProfileNames"); //String containing all WSR profile names, passed from VoiceAttack
        int ProfileNameStringIndex = ProfileNameString.IndexOf(ActivatedProfileName, StringComparison.OrdinalIgnoreCase); //Get index of requested WSR profile from within the list of available profiles (case insensitive)
        string WSRChangeResult = ""; //String that will hold the result information from the (attempted) WSR profile change
        int IncorrectProfileChangeCount;
        try { IncorrectProfileChangeCount = (int)VA.GetInt("IncorrectProfileChangeCount"); }
        catch { IncorrectProfileChangeCount = 0; }

        if (ProfileNameStringIndex &gt;= 0) //Check if requested WSR profile is an available option
        {
			string CurrentProfileName = VA.GetText("WSRCurrentProfile"); //Current WSR profile name, passed from VoiceAttack
            CurrentProfileName = ProfileNameString.Substring(ProfileNameString.IndexOf(CurrentProfileName, StringComparison.OrdinalIgnoreCase), CurrentProfileName.Length); //Redefine CurrentProfileName to ensure proper further processing and text formatting
            VA.SetText("WSRCurrentProfile", CurrentProfileName); //Store (correctly formatted) current profile name in VA text variable
            ActivatedProfileName = ProfileNameString.Substring(ProfileNameStringIndex, ActivatedProfileName.Length); //Redefine ActivatedProfileName to ensure proper further processing and text formatting
            int ProfileIndex = ProfileNameString.Substring(0, ProfileNameStringIndex).Count(f =&gt; f == ';') + 1; //Store index of ActivatedProfileName within the ProfileNameString

            if (ActivatedProfileName.Equals(CurrentProfileName) == true) //Check if requested WSR profile matches the current WSR profile
                WSRChangeResult = "Requested profile (" + ActivatedProfileName + ") already activated"; //Store result information for case where requested profile is the current profile
            else //Requested profile is not the same as the current profile (so a profile change may be possible)
            {
                int ProfileIdStringIndex;
                //string ActivatedProfileId = "";
                string ProfileIdString = VA.GetText("WSRProfileIDs"); //String containing all WSR profile IDs, passed from VoiceAttack

                if (ProfileIndex == 0)
                    ProfileIdStringIndex = 0;
                else
                {
                    ProfileIdStringIndex = ProfileIdString.IndexOf(";");
                    for (int i = 2; i &lt; ProfileIndex; i++)
                    {
                        ProfileIdStringIndex = ProfileIdString.IndexOf(";", ProfileIdStringIndex + 1);
                    }
                }
                string ActivatedProfileId = ProfileIdString.Substring(ProfileIdStringIndex+2, ProfileIdString.IndexOf(';'));

                VA.WriteToLog("4");
                string RecoProfilesRegPath = @"Software\Microsoft\Speech\RecoProfiles";

                try
                {
                    using (RegistryKey WSRProfileRoot = Registry.CurrentUser.OpenSubKey(RecoProfilesRegPath, true)) //Capture registry information for the key at the specified registry path. "using" also properly disposes RegistryKey
                        WSRProfileRoot.SetValue("DefaultTokenId", @"HKEY_CURRENT_USER\" + RecoProfilesRegPath + @"\Tokens\" + ActivatedProfileId); //Change the value of the "DefaultTokenId" in the Windows registry to the ActivatedProfileId, which will change the WSR profile to ActivatedProfileName
                }
                catch
                {
                    string ProfileChangeErrorDetail = "stuff";
                    VA.SetText("ProfileChangeErrorDetail", ProfileChangeErrorDetail);
                    return;
                }
                
                //Additional validation of profile change and checking goes here
                //try //Attempt the below "try" code and jump to "catch" if an exception (error) is encountered
                //{

                //}
                //catch (Exception ex)
                //{
                //	if (IncorrectProfileChangeCount == 1)
                //	{
                //		VA.SetInt("IncorrectProfileChangeCount", 0);
                //		WSRChangeResult = "Error during profile change"; //Store result information for case where a profile switch was attempted but an error occurred 
                //		MessageBox.Show("Error in switching profiles." + "Error message = " + ex.Message, "ERROR"); //Output additional error message to increase awareness of profile change issue
                //	}
                //	else
                //	{
                //		VA.SetInt("IncorrectProfileChangeCount", 1);
                //		return;
                //	}
                //}
                VA.SetText("WSRActivatedProfile", ActivatedProfileName); //Store (correctly formatted) activated profile name in VA text variable
                WSRChangeResult = "Activated Profile: " + ActivatedProfileName; //Store result information for case where requested profile is not the same as the current profile
            }
        }
        else
            WSRChangeResult = "Requested profile (" + ActivatedProfileName + ") not found. Profile was not changed."; //Store result information for case where requested profile is not an available option

        VA.SetText("WSRChangeResult", WSRChangeResult); //Store result of profile switch in a VA text variable
	}
