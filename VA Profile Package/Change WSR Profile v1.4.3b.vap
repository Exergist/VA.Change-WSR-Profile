namespace Change_WSR_Profile
{
    class Program
    {
        static void Main(string[] args)
        {
            //GetInfo();


            //ProcessInfo();

            //Initialize important variables
            string ProfileIdString = "";
            string ProfileNameString = "";
            string CurrentProfileName = "";
            string[] ProfileIdList, ProfileNameList;
            //string ActivatedProfileName = "profile3";

            //string WSRChangeResult = "";

            #region Retrieve WSR Profile Information from Windows Registry
            //Perform initial registry queries for current WSR profile
            RegistryKey WSRProfileRoot = Registry.CurrentUser.OpenSubKey("Software\\Microsoft\\Speech\\RecoProfiles", true); //Capture registry information for the key at the specified registry path
            string CurrentProfileData = (string)WSRProfileRoot.GetValue("DefaultTokenId"); //Extract the value data associated with the "DefaultTokenId" value name, which corresponds to the ID for the current WSR profile
            string CurrentProfileId = CurrentProfileData.Substring(CurrentProfileData.IndexOf("{")); //Extract the WSR profile ID from the value data

            //Perform initial registry queries for all available WSR profiles
            RegistryKey WSRProfiles = Registry.CurrentUser.OpenSubKey("Software\\Microsoft\\Speech\\RecoProfiles\\Tokens"); //Capture registry information for the key at the specified registry path
            ProfileIdList = WSRProfiles.GetSubKeyNames(); //Extract list of WSR profile IDs corresponding to all WSR profiles
            ProfileNameList = new string[ProfileIdList.Count()]; //Initialize size of ProfileNameList string array

            //Identify names of current and all available WSR profiles
            for (int i = 0; i &lt; ProfileIdList.Count(); i++) //Loop through all profile IDs contained within ProfileIdList
            {
                ProfileIdString += ProfileIdList[i] + "; "; //Build a string of available WSR profile names inside a single variable
                string WSRProfileName = Registry.CurrentUser.OpenSubKey("Software\\Microsoft\\Speech\\RecoProfiles\\Tokens\\" + ProfileIdList[i]).GetValue(null).ToString(); //Get actual WSR profile name associated with ProfileIdList[i]
                //ProfileNameList[i] = WSRProfileName; //Store WSRProfileName in string array
                ProfileNameString += WSRProfileName + "; "; //Build a list of WSR profile names inside a single string variable

                if (ProfileIdList[i] == CurrentProfileId) //Check if value stored in ProfileIdList[i] matches the CurrentProfileId
                    CurrentProfileName = WSRProfileName; //Store the WSR CurrentProfileName
            }
            #endregion

            string ActivatedProfileName = "Greg Samuels";
            string WSRChangeResult = "";
            

            #region Process WSR Profile Information and Change WSR Profile
            int ProfileNameStringIndex = ProfileNameString.IndexOf(ActivatedProfileName, StringComparison.OrdinalIgnoreCase); //Get index of requested WSR profile from within the list of available profiles (case insensitive)
            if (ProfileNameStringIndex &gt;= 0) //Check if requested WSR profile is an available option
            {
                //int index = Array.FindIndex(ProfileNameList, t =&gt; t.IndexOf(ActivatedProfileName, StringComparison.InvariantCultureIgnoreCase) &gt;= 0); //Store index of ActivatedProfileName within the ProfileNameList array (case insensitive comparison)
                //ActivatedProfileName = ProfileNameList[index]; //Redefine ActivatedProfileName to ensure proper further processing and text formatting
                ActivatedProfileName = ProfileNameString.Substring(ProfileNameStringIndex, ActivatedProfileName.Length); //Redefine ActivatedProfileName to ensure proper further processing and text formatting
                int ProfileIndex = ProfileNameString.Substring(0, ProfileNameStringIndex).Count(f =&gt; f == ';') + 1; //Store index of ActivatedProfileName within the ProfileNameList array (case insensitive comparison)

                if (ActivatedProfileName.Equals(CurrentProfileName) == true) //Check if requested WSR profile matches the current WSR profile (case sensitive)
                    WSRChangeResult = "Requested profile (" + ActivatedProfileName + ") already activated"; //Store result information
                else
                {
                    int ProfileIdStringIndex;
                    string ActivatedProfileId = "";
                    if (ProfileIndex == 0)
                    {
                        //ActivatedProfileId = ProfileIdString.Substring(0, ProfileIdString.IndexOf(';')-1);
                        ProfileIdStringIndex = 0;
                    }
                    else
                    {
                        ProfileIdStringIndex = ProfileIdString.IndexOf(";");
                        for (int i = 1; i &lt; ProfileIndex; i++)
                        {
                            ProfileIdStringIndex = ProfileIdString.IndexOf(";", ProfileIdStringIndex + 1);
                        }

                    }
                    ActivatedProfileId = ProfileIdString.Substring(ProfileIdStringIndex+2, ProfileIdString.IndexOf(';'));

                    //int ProfileIdStringIndex = ProfileIdString.
                    //ActivatedProfileId = ProfileIdString.Substring(ProfileNameStringIndex, ActivatedProfileName.Length); //Redefine ActivatedProfileName to ensure proper further processing and text formatting

                    //ActivatedProfileId = ProfileIdList[ProfileIndex]; //Extract the ActivatedProfileId from the ProfileIdList array using the index

                    try //Attempt the below "try" code and jump to "catch" if an exception (error) is encountered
                    {
                        WSRProfileRoot.SetValue("DefaultTokenId", "HKEY_CURRENT_USER\\Software\\Microsoft\\Speech\\RecoProfiles\\Tokens\\" + ActivatedProfileId); //Change the value of the "DefaultTokenId" in the Windows registry to the ActivatedProfileId, which will change the WSR profile to ActivatedProfileName
                        WSRChangeResult = "New profile installed: " + ActivatedProfileName; //Store result information
                    }
                    catch (Exception ex)
                    {
                        MessageBox.Show("Error in switching profiles." + "Error message = " + ex.Message, "ERROR");
                    }
                }
            }
            else
                WSRChangeResult = "Requested profile (" + ActivatedProfileName + ") not found. Profile was not changed."; //Store result information
            #endregion

            #region Output Information
            Console.WriteLine("Current WSR Profile = " + CurrentProfileName);
            Console.WriteLine("WSR Profile List = " + ProfileNameString);
            Console.WriteLine(WSRChangeResult);
            Console.WriteLine("Press any key to continue...");
            Console.ReadLine();
            #endregion
        }

        public static void GetInfo()
        {
            //Initialize important variables
            string ProfileIdString = "";
            string CurrentProfileName = "";
            string[] ProfileIdList, ProfileNameList;

            #region Retrieve WSR Profile Information from Windows Registry
            //Perform initial registry queries for current WSR profile
            RegistryKey WSRProfileRoot = Registry.CurrentUser.OpenSubKey("Software\\Microsoft\\Speech\\RecoProfiles", true); //Capture registry information for the key at the specified registry path
            string CurrentProfileData = (string)WSRProfileRoot.GetValue("DefaultTokenId"); //Extract the value data associated with the "DefaultTokenId" value name, which corresponds to the ID for the current WSR profile
            string CurrentProfileId = CurrentProfileData.Substring(CurrentProfileData.IndexOf("{")); //Extract the WSR profile ID from the value data

            //Perform initial registry queries for all available WSR profiles
            RegistryKey WSRProfiles = Registry.CurrentUser.OpenSubKey("Software\\Microsoft\\Speech\\RecoProfiles\\Tokens"); //Capture registry information for the key at the specified registry path
            ProfileIdList = WSRProfiles.GetSubKeyNames(); //Extract list of WSR profile IDs corresponding to all WSR profiles
            ProfileNameList = new string[ProfileIdList.Count()]; //Initialize size of ProfileNameList string array

            //Identify names of current and all available WSR profiles
            for (int i = 0; i &lt; ProfileIdList.Count(); i++) //Loop through all profile IDs contained within ProfileIdList
            {
                string WSRProfileName = Registry.CurrentUser.OpenSubKey("Software\\Microsoft\\Speech\\RecoProfiles\\Tokens\\" + ProfileIdList[i]).GetValue(null).ToString(); //Get actual WSR profile name associated with profileId
                ProfileNameList[i] = WSRProfileName; //Store WSRProfileName in string array
                ProfileIdString += WSRProfileName + "; "; //Build a list of WSR profile names inside a single string variable

                if (ProfileIdList[i] == CurrentProfileId) //Check if value stored in ProfileIdList[i] matches the CurrentProfileId
                    CurrentProfileName = WSRProfileName; //Store the WSR CurrentProfileName
            }
